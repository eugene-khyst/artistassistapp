{
  "name": "artistassistapp",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "lingui:extract": "lingui extract",
    "translate": "tsx translate-po.ts",
    "dev": "CF_PAGES_COMMIT_SHA=$(git rev-parse HEAD) vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "clean": "rm -rf node_modules/.vite dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:write": "prettier . --write",
    "check": "tsc --noEmit",
    "test": "npm run check && npm run lint && npm run format",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@fontsource/kalam": "^5.2.6",
    "@lingui/react": "^5.3.2",
    "@tanstack/react-query": "^5.82.0",
    "@yudiel/react-qr-scanner": "^2.3.1",
    "antd": "^5.26.4",
    "comlink": "^4.4.2",
    "dayjs": "^1.11.13",
    "file-saver": "^2.0.5",
    "idb": "^8.0.3",
    "jose": "^6.0.11",
    "onnxruntime-web": "^1.21.1",
    "print-js": "^1.6.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-compare-slider": "^3.1.0",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-sticky-box": "^2.0.5",
    "react-string-replace": "^1.1.1",
    "react-to-print": "^3.1.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@lingui/swc-plugin": "^5.5.2",
    "@lingui/vite-plugin": "^5.3.2",
    "@types/file-saver": "^2.0.7",
    "@types/gettext-parser": "^8.0.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "bing-translate-api": "^4.1.0",
    "eslint": "^9.30.1",
    "eslint-plugin-license-header": "^0.8.0",
    "eslint-plugin-lingui": "^0.10.1",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "gettext-parser": "^8.0.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "tsc-files": "^1.1.4",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^6.3.5",
    "vite-plugin-glsl": "^1.5.1",
    "vite-plugin-pwa": "^1.0.1"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{ts,tsx}": [
      "sh -c 'tsc-files --noEmit $(ls -d ./**/* | grep .d.ts) $0 $@'",
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{css,html}": "prettier --write"
  },
  "license": "AGPL-3.0-only"
}
