{
  "name": "artistassistapp",
  "version": "1.0.0",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "calc-reflectance": "ts-node --project ./src/bin/tsconfig.json ./src/bin/reflectance-calculator.ts ./static/data && prettier --write ./static/data/**/*.json",
    "validate-sets": "ts-node --project ./src/bin/tsconfig.json ./src/bin/color-set-validator.ts ./static/data",
    "start": "parcel serve src/index.html",
    "build": "parcel build src/index.html",
    "clean": "rm -rf .parcel-cache dist",
    "format": "prettier --write src",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "check": "tsc --noEmit -p ./src/tsconfig.json",
    "test": "npm run lint && npm run check",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.1",
    "@fontsource/kalam": "^5.0.8",
    "@tanstack/react-query": "^5.24.6",
    "@types/ua-parser-js": "^0.7.39",
    "antd": "^5.15.0",
    "comlink": "^4.4.1",
    "idb": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-sticky-box": "^2.0.5",
    "react-to-print": "^2.15.1",
    "ua-parser-js": "^1.0.37",
    "usehooks-ts": "^2.15.1"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.12.0",
    "@parcel/packager-raw-url": "^2.12.0",
    "@parcel/transformer-jsonld": "^2.12.0",
    "@parcel/transformer-raw": "^2.12.0",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-license-header": "^0.6.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "parcel": "^2.12.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "lint-staged": {
    "**/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{css,html}": "prettier --write"
  },
  "license": "Apache-2.0"
}
